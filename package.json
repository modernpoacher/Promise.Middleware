{
  "name": "zashiki-promise-middleware",
  "version": "1.0.2",
  "description": "Redux Promise middleware for Zashiki",
  "main": "./index.js",
  "scripts": {
    "prepublishOnly": "npm run babel",
    "prebuild": "npm run test",
    "build": "bash index.sh; npm run babel",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "pretest": "npm run lint",
    "test": "./bin/test.sh",
    "lint": "eslint src config/mocha",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/modernpoacher/Zashiki.PromiseMiddleware.git"
  },
  "keywords": [
    "redux",
    "middleware",
    "middlewares",
    "promise",
    "promises",
    "optimistic update",
    "optimistic updates",
    "async",
    "example",
    "zashiki"
  ],
  "author": "Patrick Burtchaell <patrick@pburtchaell.com> (pburtchaell.com)",
  "contributors": [
    "Thomas <iammotivated@gmail.com> (tomatao.co.uk)",
    "Jonathan Perry <modernpoacher@modernpoacher.com> (Modern Poacher Limited)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/modernpoacher/Promise.Middleware/issues"
  },
  "homepage": "https://github.com/modernpoacher/Promise.Middleware",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "html"
    ],
    "report-dir": "config/coverage"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-root-import": "^5.1.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.1",
    "eslint": "^4.10.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.2.3",
    "sinon": "^4.1.1",
    "sinon-chai": "^2.13.0"
  },
  "peerDependencies": {
    "redux": "^2.0.0 || ^3.0.0"
  },
  "dependencies": {
    "is-promise-like": "^0.1.0"
  }
}
