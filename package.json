{
  "name": "zashiki-promise-middleware",
  "version": "1.0.11",
  "description": "Redux Promise middleware for Zashiki",
  "main": "./index.js",
  "scripts": {
    "prepublishOnly": "npm run babel",
    "prebuild": "npm run clean",
    "build": "npm run babel",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "test": "bash bin/test.sh",
    "lint": "eslint src config/mocha",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "npm run lint && npm run test && npm run babel && git add lib",
    "deps": "deps",
    "deps:prod": "deps --save-prod",
    "deps:dev": "deps --save-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/modernpoacher/zashiki-promise-middleware.git"
  },
  "keywords": [
    "redux",
    "middleware",
    "middlewares",
    "promise",
    "promises",
    "optimistic update",
    "optimistic updates",
    "async",
    "example",
    "zashiki"
  ],
  "author": "Patrick Burtchaell <patrick@pburtchaell.com> (pburtchaell.com)",
  "contributors": [
    "Thomas <iammotivated@gmail.com> (tomatao.co.uk)",
    "Jonathan Perry <modernpoacher@modernpoacher.com> (Modern Poacher Limited)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/modernpoacher/zashiki-promise-middleware/issues"
  },
  "homepage": "https://github.com/modernpoacher/zashiki-promise-middleware",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "html"
    ],
    "report-dir": "config/coverage"
  },
  "dependencies": {
    "is-promise-like": "^0.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@modernpoacher/deps": "0.0.12",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "chai": "^4.2.0",
    "core-js": "^3.2.1",
    "eslint": "^6.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.5",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^3.0.0",
    "sinon": "^7.4.2",
    "sinon-chai": "^3.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
